{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "moonbit_interface",
      "camelcase": "MoonbitInterface",
      "title": "MoonBit Interface",
      "scope": "source.moonbit_interface",
      "file-types": [
        "mbti"
      ],
      "injection-regex": "^moonbit_interface$",
      "class-name": "TreeSitterMoonbitInterface"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "Apache-2.0",
    "description": "MoonBit Interface grammar for tree-sitter",
    "authors": [
      {
        "name": "Moonbit Language Team",
        "email": "support@moonbitlang.com",
        "url": "https://www.moonbitlang.com/"
      }
    ],
    "links": {
      "repository": "https://github.com/moonbitlang/tree-sitter-moonbit"
    }
  },
  "bindings": {
    "c": false,
    "go": false,
    "node": false,
    "python": false,
    "rust": false,
    "swift": false,
    "zig": false
  }
}